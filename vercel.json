{
  "version": 2,
  "builds": [
    {
      "src": "api/bfhl.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/health.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bfhl",
      "dest": "/api/bfhl.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    }
  ]
}